<h3>ユーザー検索</h3>
<%= search_form_for @search_user, url: manage_end_users_path do |f| %>
<div class="form-box">
    <%= f.search_field :last_name_or_first_name_or_last_kana_or_first_kana_cont_any, class: "form-control pull-left", :style=>"width: 67%;", placeholder: "ユーザー名で検索" %>
    <%= f.submit '検索', class: "btn btn-default", :style=> "width:33%;" %>
</div>

<% end %>

