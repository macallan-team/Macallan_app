  <div class="container">
    <div class="row">
      <div class="col-xs-9">
        <div class="">
          <h2>ユーザー情報編集ページ</h2>
            <%= form_for @end_user, url: manage_end_user_path do |f| %>
              <div class="box">

                <div class="row form-box">
                  <p><%= f.label :氏名 %></p>
                  <div class="col-xs-10"><%= f.text_field :last_name, autofocus: true, class:"form-inline", placeholder:"姓" %> <%= f.text_field :first_name, autofocus: true, class:"form-inline", placeholder:"名"%></div>
                </div>

                <div class="row form-box">
                  <p><%= f.label :氏名%><em>(カナ)</em></p>
                  <div class="col-xs-10"><%= f.text_field :last_kana, autofocus: true, class:"form-inline", placeholder:"セイ" %> <%= f.text_field :first_kana, autofocus: true, class:"form-inline", placeholder:"メイ"%></div>
                </div>

                <div class="row form-box">
                  <p><%= f.label :郵便番号 %></p>
                  <div class="col-xs-9"><%= f.text_field :postal_code, autofocus: true, class:"form-control", placeholder:"ハイフンなし" %></div>
                </div>

                <div class="row form-box">
                  <p><%= f.label :住所 %></p>
                  <div class="col-xs-9"><%= f.text_field :address, autofocus: true, class:"form-control" %></div>
                </div>

                <div class="row form-box">
                  <p><%= f.label :電話番号 %></p>
                  <div class="col-xs-9"><%= f.text_field :phone_number, autofocus: true, class:"form-control", placeholder:"ハイフンなし" %></div>
                </div>

                <div class="row form-box">
                  <p><%= f.label :メールアドレス %></p>
                  <div class="col-xs-9"><%= f.email_field :email, class:"form-control"%></div>
                </div>

                <div class="actions">
                  <div><%= f.submit "更新する", class:"btn btn-primary btn-sm" %></div>
                </div>
              <% end %>

              <div><%= link_to "戻る", :back %></div>

            </div>
        </div>
      </div>
    </div>

  </div>
</body>