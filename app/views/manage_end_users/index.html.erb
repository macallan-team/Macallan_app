<div class="container">
    <div class="row">
      <div class="col-sm-3">
        <%= render "layouts/adminsidebar" %>
      </div>
      <div class="col-sm-9">
        <h2>ユーザー一覧</h2>
        <table class="table table-striped table-responsive">
          <thead>
            <tr>
              <th><strong>ユーザー名</strong></th>
              <th><strong>電話番号</strong></th>
              <th><strong>住所</strong></th>
              <th></th>

            </tr>
          </thead>
          <tbody>
            <% @end_user.each do |f| %>
              <tr>
                <td><%= f.last_name %> <%= f.first_name %></td>
                <td><%= f.phone_number %></td>
                <td><%= f.address.truncate(25) %></td>
                <td><%= link_to "詳細", manage_end_user_path(f.id), class: "btn btn-primary"%></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>

</div>