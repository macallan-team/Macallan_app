<%= form_with model: @address,url: end_user_addresses_path, local: true do |f| %>
<div class="form-box">
    <label>氏名</label>
    <%= f.text_field :name, class:"form-control", placeholder: "全角??文字以内"%>
</div>
<div class="form-box">
    <label>住所</label>
    <%= f.text_field :address, class:"form-control", placeholder: "全角50文字以内" %>
</div>
<div class="form-box">
    <label>郵便番号</label>
    <%= f.text_field :postal_code, class:"form-control", placeholder: "ハイフンなし" %>
</div>
<div class="form-box">
    <label>電話番号</label>
    <%= f.text_field :phone_number, class:"form-control",placeholder: "ハイフンなし" %>
</div>
<div class="form-box">
    <%= f.submit "登録", class:"btn btn-default center-block"%>
</div>
<% end %>